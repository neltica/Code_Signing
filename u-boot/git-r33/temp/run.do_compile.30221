oe_runmake() {
	bbnote make -j 2 CROSS_COMPILE=powerpc-fsl_networking-linux-gnuspe- CC="powerpc-fsl_networking-linux-gnuspe-gcc  --sysroot=/home/sangmin/QorIQ-SDK-V1.4-20130625-yocto/build_p2020rdb_release/tmp/sysroots/p2020rdb" "$@"
	make -j 2 CROSS_COMPILE=powerpc-fsl_networking-linux-gnuspe- CC="powerpc-fsl_networking-linux-gnuspe-gcc  --sysroot=/home/sangmin/QorIQ-SDK-V1.4-20130625-yocto/build_p2020rdb_release/tmp/sysroots/p2020rdb" "$@" || die "oe_runmake failed"

}

die() {
	bbfatal "$*"

}

bbnote() {
	echo "NOTE: $*"

}

bbfatal() {
	echo "ERROR: $*"
	exit 1

}

cd /home/sangmin/QorIQ-SDK-V1.4-20130625-yocto/build_p2020rdb_release/tmp/work/p2020rdb-fsl_networking-linux-gnuspe/u-boot/git-r33/git
